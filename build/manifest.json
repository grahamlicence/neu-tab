{
   "manifest_version": 2,
   "name": "Neu Tab",
   "version": "0.3.1",
   "description": "Alternative new tab screen.",
   "icons": {
      "128": "assets/images/icon128.png",
      "16": "assets/images/icon16.png",
      "96": "assets/images/icon96.png"
   },
   "chrome_url_overrides": {
      "newtab": "index.html"
   },
   "permissions": [
      "bookmarks", 
      "geolocation", 
      "http://maps.googleapis.com/", 
      "http://*.yahooapis.com/",
      "http://feeds.bbci.co.uk/"
   ]
}
